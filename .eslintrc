{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "js"
    ],
    "rules": {
        "no-underscore-dangle": [
        "error",
          {
              "allow": ["_id"]
          }
      ]
    }
}
